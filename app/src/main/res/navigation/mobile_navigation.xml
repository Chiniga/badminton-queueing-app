<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_queue">

    <fragment
        android:id="@+id/navigation_queue"
        android:name="com.roda.paqueue.ui.queue.QueueFragment"
        android:label="@string/title_queue"
        tools:layout="@layout/fragment_queue" />

    <fragment
        android:id="@+id/navigation_players"
        android:name="com.roda.paqueue.ui.players.PlayersFragment"
        android:label="@string/title_players"
        tools:layout="@layout/fragment_players" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.roda.paqueue.ui.settings.SettingsFragment"
        android:label="@string/title_settings"
        tools:layout="@layout/fragment_settings" />
</navigation>
